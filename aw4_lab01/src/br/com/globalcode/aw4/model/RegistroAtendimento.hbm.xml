<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="br.com.globalcode.aw4.model.RegistroAtendimento" table="tb_atendimento">
    <id column="id_atendimento" name="id" type="java.lang.Integer">
      <generator class="identity"/>
    </id>
    <property column="tx_email_visitante" name="emailVisitante" type="string"/>
    <property column="tx_duvida" name="textoDuvida" type="string"/>
    <property column="dt_registro" name="dataRegistro" type="timestamp"/>
    <property column="dt_resposta" name="dataResposta" type="timestamp"/>
    <property column="tx_resposta" name="textoResposta" type="string"/>
  </class>
</hibernate-mapping>
